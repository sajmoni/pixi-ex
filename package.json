{
  "name": "pixi-ex",
  "version": "0.0.7",
  "description": "Pixi extended - Utility library for Pixi.js",
  "main": "./dist/index.js",
  "files": [
    "/dist/*"
  ],
  "author": "Simon Lagos <simon.lagos.sallhed@gmail.com>",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sajmoni/pixi-ex.git"
  },
  "scripts": {
    "clean": "rm -f pixi-ex.tgz",
    "go": "yarn clean && yarn build && yarn pack --filename pixi-ex.tgz && cd example && yarn refresh && cd -",
    "build": "rm -rf dist && parcel build ./index.js --no-cache",
    "lint": "eslint index.js",
    "release": "yarn clean && yarn audit 6& yarn build && np",
    "test": "ava"
  },
  "devDependencies": {
    "@babel/preset-env": "7.9.5",
    "@babel/register": "7.9.0",
    "ava": "3.7.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.20.1",
    "np": "6.2.1",
    "parcel-bundler": "1.12.4"
  },
  "dependencies": {
    "core-js": "3.6.5"
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  }
}
