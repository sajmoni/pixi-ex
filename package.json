{
  "name": "pixi-ex",
  "version": "0.0.5",
  "description": "Pixi extended - Utility library for Pixi.js",
  "main": "./dist/index.js",
  "files": [
    "/dist/*"
  ],
  "author": "Simon Lagos <simon.lagos.sallhed@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "clean": "rm -f pixi-ex.tgz",
    "build-test": "yarn clean && yarn build && yarn pack --filename pixi-ex.tgz && cd example && yarn refresh && cd -",
    "build": "rm -rf dist && parcel build ./index.js --no-cache",
    "lint": "eslint index.js",
    "release": "yarn build && np",
    "test": "ava"
  },
  "devDependencies": {
    "@babel/preset-env": "7.8.4",
    "@babel/register": "7.8.3",
    "ava": "3.3.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.20.1",
    "np": "6.0.0",
    "parcel-bundler": "1.12.4",
    "pixi.js": "5.2.1"
  },
  "dependencies": {
    "core-js": "3.6.4"
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  }
}
