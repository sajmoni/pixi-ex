{
  "name": "pixi-ex",
  "version": "0.0.7",
  "description": "Pixi extended - Utility library for Pixi.js",
  "main": "./dist/index.js",
  "files": [
    "/dist/*"
  ],
  "author": "Simon Lagos <simon.lagos.sallhed@gmail.com>",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sajmoni/pixi-ex.git"
  },
  "scripts": {
    "clean": "rm -f pixi-ex.tgz",
    "go": "yarn clean && yarn build && yarn pack --filename pixi-ex.tgz && cd example && yarn refresh && yarn start",
    "build": "rm -rf dist && rollup --config rollup.config.js",
    "release": "yarn clean && yarn audit 6& yarn build && np",
    "test": "xo && ava"
  },
  "prettier": {
    "trailingComma": "all",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "bracketSpacing": true
  },
  "xo": {
    "prettier": true,
    "env": [
      "es2020",
      "browser"
    ],
    "parser": "@typescript-eslint/parser",
    "rules": {
      "@typescript-eslint/prefer-readonly-parameter-types": "off",
      "unicorn/filename-case": "off",
      "capitalized-comments": "off",
      "dot-notation": "off"
    }
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/register": "7.9.0",
    "@rollup/plugin-typescript": "4.1.1",
    "ava": "3.8.2",
    "np": "6.2.3",
    "pixi.js": "5.2.4",
    "rollup": "2.10.0",
    "typescript": "3.9.2",
    "xo": "0.30.0"
  },
  "peerDependencies": {
    "pixi.js": "5.x"
  },
  "dependencies": {
    "core-js": "3.6.5"
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  }
}
