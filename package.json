{
  "name": "pixi-ex",
  "version": "0.0.11",
  "description": "Pixi extended - Utility library for Pixi.js",
  "main": "./dist/index.js",
  "files": [
    "/dist/*"
  ],
  "author": "Simon Lagos <simon.lagos.sallhed@gmail.com>",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sajmoni/pixi-ex.git"
  },
  "scripts": {
    "clean": "rm -f pixi-ex.tgz",
    "go": "yarn clean && yarn build && yarn pack --filename pixi-ex.tgz && cd example && yarn refresh && yarn start",
    "qa": "tsc && xo --fix",
    "build": "rm -rf dist && rollup --config rollup.config.js",
    "release": "yarn clean && yarn audit && yarn test && yarn build && np",
    "test": "ava"
  },
  "prettier": {
    "trailingComma": "all",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "bracketSpacing": true
  },
  "xo": {
    "prettier": true,
    "env": [
      "es2020",
      "browser"
    ],
    "parser": "@typescript-eslint/parser",
    "rules": {
      "@typescript-eslint/prefer-readonly-parameter-types": "off",
      "unicorn/filename-case": "off",
      "capitalized-comments": "off",
      "dot-notation": "off"
    }
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-typescript": "7.12.7",
    "@babel/register": "7.12.10",
    "@rollup/plugin-typescript": "8.1.0",
    "ava": "3.14.0",
    "canvas": "2.6.1",
    "jsdom": "16.4.0",
    "np": "7.1.0",
    "pixi.js": "5.3.6",
    "rollup": "2.35.1",
    "typescript": "4.1.3",
    "xo": "0.36.1"
  },
  "dependencies": {},
  "ava": {
    "require": [
      "./setupTests.js"
    ],
    "extensions": [
      "js",
      "ts"
    ]
  }
}
