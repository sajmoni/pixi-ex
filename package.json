{
  "name": "pixi-ex",
  "version": "0.1.0",
  "description": "Pixi extended - Utility library for Pixi.js",
  "main": "dist/index.js",
  "files": [
    "/dist"
  ],
  "author": "Simon Lagos <simon.lagos.sallhed@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sajmoni/pixi-ex.git"
  },
  "scripts": {
    "go": "npm run build && preview && npm test",
    "release": "npm run build && np --no-tests",
    "test": "ava",
    "app": "npm run build && preview && cd app && npm run build && npm run start",
    "build": "rm -rf dist && tsc"
  },
  "ava": {
    "require": [
      "esbuild-runner/register"
    ],
    "extensions": [
      "ts"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "esbuild": "^0.13.2",
    "esbuild-runner": "^2.2.1",
    "np": "^7.5.0",
    "package-preview": "^4.0.0",
    "prettier": "^2.4.1",
    "serve": "^12.0.1",
    "typescript": "^4.4.3"
  },
  "peerDependencies": {
    "pixi.js": "6.x"
  }
}
